
<?init class="com.pennant.authentication.Authentication"?>
<?page title="Customerorders" contentType="text/html;charset=UTF-8"?>

<zk>

<window  id="myordersview" use="com.pennant.customer.Myorderpopctrl"  order="${arg.order}" >

  <div id="Custmoerord"  width="500px"  >
     <vbox >
		<hbox>
			<label value="My Orders" style="font-family: Georgia, serif;font-size: 30px;color:white "/>
		</hbox>
		  <space width="10px"/>
         <listbox id="myorder" hflex="min" vflex="min"  width="450px" multiple="true" >
           <listhead>
             
